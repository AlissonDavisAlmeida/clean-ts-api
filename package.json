{
  "name": "main-project",
  "version": "1.2.3",
  "main": "index.js",
  "author": "alisson <alissoneatena1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "build": "tsc && tsc-alias",
    "up:compose": "yarn build && docker compose up --build -d",
    "eslint": "eslint",
    "precommit": "lint-staged",
    "test": "jest --passWithNoTests --noStackTrace --runInBand",
    "dev": "ts-node --transpiler sucrase/ts-node-plugin src/main/server.ts",
    "test:verbose": "jest --passWithNoTests",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:unit": "yarn test:verbose --watchAll -c jest-unit.config.ts",
    "test:integration": "yarn test --watchAll -c jest-integration.config.ts"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint  --fix",
      "yarn test:staged"
    ]
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.3.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.13",
    "@types/supertest": "^2.0.16",
    "@types/validator": "^13.11.10",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^36.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^13.3.0",
    "mockdate": "^3.0.5",
    "sucrase": "^3.35.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "5.9.2",
    "validator": "^13.12.0"
  },
  "packageManager": "yarn@4.2.2"
}
